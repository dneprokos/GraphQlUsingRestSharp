<?xml version="1.0" encoding="utf-8"?>
<!-- File name extension must be .runsettings -->
<RunSettings>
  <RunConfiguration>
    <ResultsDirectory>.\Results</ResultsDirectory>
    <DisableParallelization>false</DisableParallelization>
  </RunConfiguration>

  <TestRunParameters>
    <!--Application data configuration-->
    <Parameter name="countriesApiBaseUrl" value="https://countries.trevorblades.com/graphql" />
    <!--Logger settings-->
    <Parameter name="logLevel" value="Debug" />
    <Parameter name="logFile" value="" /> 
  </TestRunParameters>
</RunSettings>